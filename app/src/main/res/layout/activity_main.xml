            android:ems="10"
            android:inputType="textPersonName"
            android:text="Name"
            android:textSize="20sp"
            tools:layout_editor_absoluteX="187dp"
            tools:layout_editor_absoluteY="308dp" />

        <EditText
            android:id="@+id/editText6"
            android:layout_width="wrap_content"
