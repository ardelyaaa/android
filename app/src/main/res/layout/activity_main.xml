            android:layout_width="133dp"
            android:layout_height="57dp"
            android:backgroundTint="@android:color/darker_gray"
            android:text="OK"
            tools:layout_editor_absoluteX="228dp"
            tools:layout_editor_absoluteY="594dp" />

        <TextView
